<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
    <style>
        table, th, td {
          border: 1px solid black;
        }
    </style>
<body>
    <table style="width:100%">
        <tr>
          <th>First Name</th>
          <th>Last Name</th>
          <th>Email</th>
          <th>Actions</th>
        </tr>

        <% for(var i=0; i < dataTable.length; i++) { %>
            <tr>
              <td><%= dataTable[i].firstName %></td>
              <td><%= dataTable[i].lastName %></td>
              <td><%= dataTable[i].email %></td>
              <td>
                <a href="/edit-user?id=<%= dataTable[i].id %>">Edit</a>
                <a href="/delete-user?id=<%= dataTable[i].id %>">Delete</a>
              </td>
            </tr>
        <% } %>

        
        
      </table>
</body>
</html>